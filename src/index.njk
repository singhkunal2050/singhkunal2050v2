    <section class="writing-section">
      
       <h1 class="head_text" style="text-align: center; padding-bottom: 40px;"><span
          class="head_text rainbow">Recent</span> Blogs</h1>

        <div class="blog-list-container ">
          {%- for post in collections.post | reverse -%}
              {% if loop.index0 < 3 %}    
                <a href="{{ post.url | url }}" class="blog-link">
                    <div class="blog-card">
                      <h1 class="blog-title subhead_text">{{ post.data.title}}</h1>
                      <div class="blog-hashtags">
                        {% set comma = joiner() %}
                        {% for tag in post.data.tags -%}
                          {% if tag !== 'post' %}
                            {{ comma() }}
                            <p href="#">#{{ tag }}</p>
                          {% endif %}
                        {%- endfor %}
                      </div>
                      <p class="blog-description">{{post.data.description}}</p>
                      <div class="blog-details">
                        <p class="blog-date">{{ post.data.date | postDate }}</p>
                      </div>
                    </div>
                </a>
              {% endif %}
          {%- endfor  -%}
        </div>

        <a href="/blogs" class="btn-link" >All Blogs >> </a>
        
    </section>


